{
    "env": {
        "sample_name": "library",
        "sample_path": "./samples/${sample_name}"
    },
    "configurations": [
        {
            "name": "x86",
            "compileCommands": "${workspaceRoot}/build/compile_commands.json",
            "intelliSenseMode": "linux-gcc-x64",
